CASI DI ERRORE-1: Errore nel chiamare un metodo di una classe

class Numero (int x){
        void uno() x;
        bool due() true;
    }

class Ciao extends Numero(bool y){
        bool tre() false;
}

class Ciaone extends Ciao(Numero a){
        Numero quattro() new Numero(4);
};
    let
        Numero num = new Numero(2);
        Ciao ciao= new Ciao(1,false);
    in
if ciao.tre() then{num.due()} else{ciao.tre()};
